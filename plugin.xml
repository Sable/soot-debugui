<extension
      point="org.eclipse.debug.core.launchModes">
   <launchMode
         label="Analyze"
         launchAsLabel="Heros Analysis"
         mode="heros">
   </launchMode>
</extension>
<extension
      point="org.eclipse.debug.core.launchDelegates">
   <launchDelegate
         delegate="heros.debugui.launching.HerosDebugLaunchDelegate"
         id="heros.launchDelegate.java"
         modes="heros"
         name="Analyze Java Appliction with Heros"
         type="org.eclipse.jdt.launching.localJavaApplication">
   </launchDelegate>
</extension>
<extension
      point="org.eclipse.debug.ui.launchGroups">
   <launchGroup
         bannerImage=""
         image=""
         id="heros.launchGroups.analyze"
         label="Analyze"
         mode="heros"
         title="TITLE: Analyze">
   </launchGroup>
</extension>
<extension
      point="org.eclipse.debug.ui.launchConfigurationTabGroups">
   <launchConfigurationTabGroup
         class="heros.debugui.HerosLocalJavaApplicationTabGroup"
         id="heros.launchConfigurationTabGroups.java"
         type="org.eclipse.jdt.launching.localJavaApplication">
      <launchMode
            mode="heros">
      </launchMode>
   </launchConfigurationTabGroup>
</extension>
 
<extension
      point="org.eclipse.ui.actionSets">
   <actionSet
         id="de.bodden.tamiflex.actionSet"
         label="Heros"
         visible="true">
      <action
            label="Analyze History"
            style="pulldown"
            class="heros.debugui.action.AnalyzeMenuAction"
            menubarPath="org.eclipse.ui.run/runGroup"            
            id="heros.actions.analyzeMenu">
      </action>
      <action
            class="heros.debugui.action.AnalyzeAction"
            hoverIcon=""
            disabledIcon=""
            icon=""
            id="heros.actions.analyze"
            label="Analyze..."
            menubarPath="org.eclipse.ui.run/runGroup"
            tooltip="Analyze">
      </action>
      <action
      		id="heros.debugui.AnalyzeHistoryDropDownAction"
            toolbarPath="debug.ext"
            class="heros.debugui.action.AnalyzeToolbarAction"
            icon="icons/start_task.gif"
            label="Analyze"
            style="pulldown">
      </action>
      <action
      		id="heros.debugui.NextFlow"
            toolbarPath="debug.ext"
            class="heros.debugui.action.NextFlowToolbarAction"
            icon="icons/shift_r_edit.gif"
            label="Next FLow">
      </action>
      <action
      		id="heros.debugui.PreviousFlow"
            toolbarPath="debug.ext"
            class="heros.debugui.action.PreviousFlowToolbarAction"
            icon="icons/shift_l_edit.gif"
            label="Previous FLow">
      </action>
     </actionSet>
</extension>
</plugin>
